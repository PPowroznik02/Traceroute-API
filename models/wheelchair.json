{
    "speed": [
        {
            "if": "true",
            "limit_to": 3
        },
        {
            "if": "road_class == PRIMARY",
            "limit_to": 4
        },
        {
            "if": "road_class == SECONDARY",
            "limit_to": 4
        },
        {
            "if": "surface == ASPHALT",
            "limit_to": 5
        },
        {
            "if": "surface == PAVED",
            "limit_to": 5
        },
        {
            "if": "surface == GRAVEL",
            "limit_to": 3
        },
        {
            "if": "surface == DIRT",
            "limit_to": 1
        },
        {
            "if": "smoothness == EXCELLENT",
            "limit_to": 5
        },
        {
            "if": "smoothness == GOOD",
            "limit_to": 4
        },
        {
            "if": "smoothness == INTERMEDIATE",
            "limit_to": 3
        },
        {
            "if": "smoothness == BAD",
            "limit_to": 1
        },
        {
            "if": "max_slope >= 6",
            "limit_to": 1
        },
        {
            "if": "max_slope < 6",
            "limit_to": 3
        }
      ],

    "priority": [
        {
            "if": "true",
            "multiply_by": "1"
        },
        {
            "if": "road_access == PRIVATE",
            "multiply_by": 0
        },
        {
            "if": "road_class == PRIMARY",
            "multiply_by": 0.3
        },
        {
            "if": "road_class == SECONDARY",
            "multiply_by": 0.7
        },
        {
            "if": "road_class == PEDESTRIAN",
            "multiply_by": 1
        },
        {
            "if": "road_class == STEPS",
            "multiply_by": 0
        },
        {
            "if": "surface == ASPHALT",
            "multiply_by": 1.0
        },
        {
            "if": "surface == PAVED",
            "multiply_by": 1.0
        },
        {
            "if": "surface == GRAVEL",
            "multiply_by": 0.5
        },
        {
            "if": "surface == DIRT",
            "multiply_by": 0.3
        },
        {
            "if": "smoothness == EXCELLENT",
            "multiply_by": 1.0
        },
        {
            "if": "smoothness == GOOD",
            "multiply_by": 0.8
        },
        {
            "if": "smoothness == INTERMEDIATE",
            "multiply_by": 0.5
        },
        {
            "if": "smoothness == BAD",
            "multiply_by": 0.1
        },
        {
            "if": "max_slope >= 6",
            "multiply_by": 0.2
        },
        {
            "if": "max_slope < 6",
            "multiply_by": 1.0
        },
        {
            "else": "",
            "multiply_by": 1
        }
    ]

}
